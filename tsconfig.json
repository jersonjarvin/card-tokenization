{
  "compilerOptions":{
     "target":"es2019",
     "lib":[
        "es2019",
        "es2020",
        "es2021",
        "esnext"
     ],
     "experimentalDecorators":true,
     "emitDecoratorMetadata":true,
     "module":"commonjs",
     "rootDir":"./",
     "moduleResolution":"node",
     "esModuleInterop": true,
     "baseUrl":"./",
     "paths":{
        "@common/*":[
           "src/common/*"
        ],
        "@controllers/*":[
           "src/controllers/*"
        ],
        "@/*":[
           "src/*"
        ]
     },
     "typeRoots":[
        "../node_modules/@types",
        "node_modules/@types",
        "src/types"
     ],
     "types":[
        "reflect-metadata",
        "@types/jest"
     ],
     "allowJs":true,
     "declaration":false,
     "sourceMap":true,
     "outDir":"./dist",
     "forceConsistentCasingInFileNames":true,
     "strict":true,
     "noImplicitAny":true,
     "strictPropertyInitialization":false,
     "noUnusedLocals":true,
     "noUnusedParameters":true,
     "skipLibCheck":true
  },
  "exclude":[
     "node_modules",
     "dist"
  ]
}